
@{
    ViewData["Title"] = "Account";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="backgroundaccount">
        <img src="https://fy2205storagerafaelpalo.blob.core.windows.net/fy2205images/backgroundaccount.jpg" width="2000" height="500" />
    </div>
    <div class="loginbox">
        <img src="https://fy2205storagerafaelpalo.blob.core.windows.net/fy2205images/login.png" class="loginlogo" />
        <h1>Login Here</h1>
        <form>
            <p>Email</p>
            <input type="text" name="" placeholder="Enter Email" />
